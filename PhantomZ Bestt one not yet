		if (v631:IsA("BlurEffect") or v631:IsA("SunRaysEffect") or v631:IsA("ColorCorrectionEffect") or v631:IsA("BloomEffect") or v631:IsA("DepthOfFieldEffect")) then
			v631.Enabled = false;
		end
	end
end});
local v126 = v131:AddToggle("Config", {Title="Night Mode",Description="Toggles Night Mode",Default=false});
v126:OnChanged(function(v488)
	Nightmode = v488;
end);
local v126 = v131:AddToggle("Config", {Title="Shaders",Description="Toggles Shaders",Default=true});
v126:OnChanged(function(v489)
	ShadersEffect = v489;
end);
local v132 = v123.Config:AddSection("Player");
